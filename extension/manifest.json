{
  "name": "Auto branch delete for Github",
  "version": "1.0.1",
  "icons": {
    "16": "/img/icons/icon_16.png",
    "48": "/img/icons/icon_48.png",
    "128": "/img/icons/icon_128.png"
  },
  "description": "Automatically deletes branch after PR is merged.",
  "permissions": [
    "webRequest",
    "*://*.github.com/"
  ],
  "background": {
    "scripts": [
      "/js/background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "https://github.com/*"
      ],
      "js": [
        "/js/content_script.js"
      ]
    }
  ],
  "manifest_version": 2
}
